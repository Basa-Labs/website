<div id="wps-launcher-home" class="wps-full-wrapper light-theme">
    <!-- MAIN WRAPPER -->
    <div class="wps-main-wrapper">
        {{{sidebar}}}
        <!-- WPS CONTENT -->
        <div class="wps-content-page">
            {{{main-nav}}}
            {{{message}}}
            <!-- GRID -->
            <div class="wps-dashboard-grid">
                {{{call-to-action}}}
                <div class="wps-cards">
                    <div class="wps-card">
                        <!-- NFT column -->
                        <div class="wps-panel-aaa">
                            <div class="wps-panel wps-panel-content">
                                <div class="wps-column-box wps-column-box-inner" style="margin-top: 0">
                                    <div class="wps-colBox-title-lg"><img src="{{path-to-launcher}}img/nfts-logo.svg">{{text-nfts}}</div>
                                    <div class="wps-smartContracts-table card wps-table"  style="padding-bottom: 120px; padding-top: 30px">
                                        <div id="wps-launcher-home-nfts">
                                            <img src="{{path-to-launcher}}img/not-found.png" class="wps-center-icon">
                                            <p class="wps-small-text-center">{{no-smart-contract-found}}</p>
                                        </div>
                                    </div>
                                    <div style="float: left;width: auto;max-width: auto;">
                                        <a href="{{nft_manage_link}}" class="wps-colBox-seeAll" style="width: auto;">
                                            {{create-nft}}
                                            &raquo;
                                        </a>
                                    </div>
                                    <div id="wps-see-all-nft" class="wps-colBox-cta" style="display: none;"><a href="{{dashboard-nft}}" class="wps-colBox-seeAll">{{text-see-all}}&raquo;</a></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="wps-card">
                        <!-- STAKINGS column -->
                        <div class="wps-panel-aaa">
                            <div class="wps-panel wps-panel-content">
                                <div class="wps-column-box wps-column-box-inner" style="margin-top: 0">
                                    <div class="wps-colBox-title-lg"><img src="{{path-to-launcher}}img/stakings-logo.svg">{{text-stakings}}</div>
                                    <div class="wps-smartContracts-table card wps-table" style="padding-bottom: 120px; padding-top: 30px">
                                        <div id="wps-launcher-home-stakes" >
                                            <img src="{{path-to-launcher}}img/not-found.png" class="wps-center-icon">
                                            <p class="wps-small-text-center">{{no-smart-contract-found}}</p>    
                                        </div>
                                    </div>
                                    <div style="float: left;width: auto;max-width: auto;">
                                        <a href="{{staking_manage_link}}" class="wps-colBox-seeAll" style="width: auto;">
                                            {{create-stake}}
                                            &raquo;
                                        </a>
                                    </div>
                                    <div id="wps-see-all-stakings" class="wps-colBox-cta" style="display: none;"><a href="{{dashboard-stakes}}" class="wps-colBox-seeAll">{{text-see-all}}&raquo;</a></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="wps-card">
                        <!-- TOKENS column -->
                        <div class="wps-panel-aaa">
                            <div class="wps-panel wps-panel-content">
                                <div class="wps-column-box wps-column-box-inner" style="margin-top: 0">
                                    <div class="wps-colBox-title-lg"><img src="{{path-to-launcher}}img/tokens-logo.svg">{{text-tokens}}</div>
                                    <div class="wps-smartContracts-table card wps-table" style="padding-bottom: 120px; padding-top: 30px">
                                        <div id="wps-launcher-home-coin" >
                                            <img src="{{path-to-launcher}}img/not-found.png" class="wps-center-icon">
                                            <p class="wps-small-text-center">{{no-smart-contract-found}}</p>    
                                        </div>
                                    </div>
                                    <div style="float: left;width: auto;max-width: auto;">
                                        <a href="{{token_manage_link}}" class="wps-colBox-seeAll" style="width: auto;">
                                            {{create-coin}}
                                            &raquo;
                                        </a>
                                    </div>
                                    <div id="wps-see-all-coin" class="wps-colBox-cta" style="display: none;"><a href="{{dashboard-coin}}" class="wps-colBox-seeAll">{{text-see-all}}&raquo;</a></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div> 
</div>

<span id="path-to-launcher" style="display: none;">{{path-to-launcher}}</span>
{{=<% %>=}}
<div id="wps-row-template" style="display: none;">
    <p>
        <a id="wps-name-link" href="{{link}}" class="wps-title-link">{{title}}</a>
        <button class="wps-edit-button wps-table-qr">
            <a href="{{link-edit}}">
                {{edit-label}}
            </a>
        </button>    
    </p>
    <p class="wps-centered">
        <span class="wps-tag">
            <span class="wps-tag-label">Network</span>
            <span class="wps-tag-value">  
                <span style="color: {{color-network}}; margin-right: 10px">⬤</span> 
                {{network}}
            </span>
        </span>
    </p>
    <p class="wps-centered">
        <span class="wps-tag">
            <span class="wps-tag-label">Flavor</span>
            <span class="wps-tag-value">  
                <span class="wps-bg-{{color-flavor}} color">⬤</span> 
                {{flavor}}
            </span>
        </span>
    </p>
    <p class="wps-centered">
        Smart Contract
    </p>
    <p class="wps-centered">
        <button class="wps-table-qr">
            <img id="wpsc-qr-2-{{id}}" src="{{path-to-launcher}}img/copy-solid-icon.svg" onclick="copyToClipboard('{{contract}}', 'wpsc-qr-2-{{id}}')">
        </button>
        <a href="{{etherscan}}address/{{contract}}" target="_blank" class="wps-a-contract">{{contract-short}}</a>
        <button class="wps-table-qr">
            <img src="{{path-to-launcher}}img/qrcode-solid.svg">
            <div class="wps-tooltip">
            <img src="{{qr}}">
            <div class="wps-tooltip-bottom-content">{{contract}}</div>
            </div>
        </button>
    </p>
    <a href="{{link}}" class="wps-cta wps-center wps-mini-text" style="margin: 30px auto">
        View
        <img src="{{path-to-launcher}}img/right-arrow-white.png" class="wps-arrow">
    </a>

</div>
<%={{ }}=%>

{{=<% %>=}}
<div id="wps-row-template-not-found" style="display: none;">
    <p class="wps-small-text-center">{{no-smart-contract-found}}</p>
</div>
<%={{ }}=%>
